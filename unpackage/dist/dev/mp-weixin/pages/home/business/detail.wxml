<view class="content"><view class="cu-form-group"><view class="title">姓名</view><view class="color20 font28 w-70 t-align-r">{{businessOrderDetail.name}}</view></view><view class="cu-form-group"><view class="title">订单编号</view><view class="color37 font28 w-70 t-align-r">{{businessOrderDetail.order_id}}</view></view><view class="cu-form-group"><view class="title">下单时间</view><view class="color37 font28">{{$root.m0}}</view></view><view class="cu-form-group"><view class="title">订单状态</view><view class="{{[' font28',businessOrderDetail.status?'color20':'colore6']}}">{{''+(businessOrderDetail.status?'已支付':'未支付')}}</view></view><view class="cu-form-group"><view class="title">上菜状态</view><view class="align-center"><view class=" font28" style="{{'color:'+(businessOrderDetail.serve_status?'#1abc9c':'red')+';'}}">{{''+(businessOrderDetail.serve_status?'已送达':'未送达')}}</view><switch style="margin-left:10px;" checked="{{businessOrderDetail.serve_status}}" data-event-opts="{{[['change',[['selectDishStatusChange',['$event','$0'],['businessOrderDetail']]]]]}}" bindchange="__e"></switch></view></view><view class="cu-form-group"><view class="title">订单内容</view><view class="w-70 color37 font28 w-70 t-align-r" style="white-space:pre-wrap;text-align:right;">{{$root.m1}}</view></view><view class="cu-form-group"><view class="title">收款</view><view class="color20 font28 w-70 t-align-r">{{businessOrderDetail.total/100}}</view></view><view class="cu-form-group"><view class="title">电话</view><view class="color20 font28">{{''+(businessOrderDetail.phone?businessOrderDetail.phone:'无')}}</view></view><view class="cu-form-group"><view class="title">地址</view><view class="color37 font28 w-70 t-align-r">{{''+(businessOrderDetail.address?businessOrderDetail.address:'')+"-"+(businessOrderDetail.detail_address?businessOrderDetail.detail_address:'')}}</view></view></view>